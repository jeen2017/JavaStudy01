package com.bit.arraylist;

import java.util.ArrayList;

public class ArrayListLotto {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 정수객체를 갖는 ArrayList
		ArrayList<Integer> lottos = new ArrayList<Integer>();
		
		for(int i = 0 ; i < 45 ; i++){
			lottos.add(i+1);
		}

	}

}
